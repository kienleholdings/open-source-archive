{
  "name": "@kienleholdings/zephyr-nextjs",
  "version": "3.0.0",
  "description": "Some Zephyr Components with Better NextJS Support",
  "prettier": "@kienleholdings/prettierrc",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --inject ./react-import.js --format cjs,esm --sourcemap --dts --splitting",
    "dev": "ladle serve & pnpm run build --watch",
    "lint": "eslint --cache src/**",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kienleholdings/technologies.git"
  },
  "keywords": [
    "Kienle Holdings",
    "tailwindcss",
    "zephyr"
  ],
  "author": "Kienle Technologies <hello@kienle.tech> (https://www.kienle.tech)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kienleholdings/technologies/issues"
  },
  "homepage": "https://github.com/kienleholdings/technologies/tree/main/packages/zephyr-nextjs#readme",
  "devDependencies": {
    "@kienleholdings/eslint-config-react": "workspace:*",
    "@kienleholdings/prettierrc": "workspace:*",
    "@kienleholdings/tsconfigs": "workspace:*",
    "@kienleholdings/zephyr-core": "workspace:*",
    "@kienleholdings/zephyr-react": "workspace:*",
    "@ladle/react": "^0.15.0",
    "@types/react": "^18.0.6",
    "autoprefixer": "^10.4.5",
    "eslint": "^8.14.0",
    "next": "^12.1.6",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@kienleholdings/zephyr-core": ">= 3.0.0 < 4",
    "@kienleholdings/zephyr-react": ">= 3.0.0 < 4",
    "next": ">= 12.0.0 < 13",
    "react": ">= 18.0.0 < 19",
    "react-dom": ">= 18.0.0 < 19"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  }
}
