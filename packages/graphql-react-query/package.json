    "@kienleholdings/prettierrc": "workspace:*",
    "@kienleholdings/tsconfigs": "workspace:*",
    "graphql": "^16.4.0",
    "prettier": "2.6.2",
    "react": "^18.1.0",
    "react-query": "^3.39.0",
    "tsup": "^5.12.6",
